  public static void wypisz(String rok, String miesiac)
    {
      String napis=miesiac+"."+rok+" " to " " ";
      int mies = Integer.parseInt(miesiac);
      mies/=4;
      if (mies ==0)
        napis += "I";
        else if (mies == 1)
          napis += "II"
         else if ...
      
    }
    
    
  ZADANIE 3:
  
  // wyrzucic void
  
  Student{
    main{
      int rok;
      if (czyPrzestepny(rok))
      wypisz jest przestepny
          else nie jest przestepny
  }
  
  
  public static bolean czyPrzestepny(int rok){
    bolean przestepnosc=false;
    if((rok%4==0 && rok/100!=0) || rok%400=0)
      przestepnosc=true;
    return przestepnosc;
  
  }
